{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [{
    "name": "firebaseNotesApi",
    "urls": [
      "https://notas-b6f9f.firebaseio.com/**"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxAge": "1h",
      "maxSize": 100,
      "timeOut": "3s"
    }

  }]
}
